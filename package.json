{
  "name": "team-sport-management-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "type-check": "tsc -b --noEmit",
    "validate": "npm run type-check && npm run lint",
    "supabase:check": "bash scripts/check-supabase-env.sh --auto-unlink",
    "supabase:start": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase start",
    "supabase:stop": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase stop",
    "supabase:status": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase status",
    "supabase:studio": "open http://127.0.0.1:54323",
    "supabase:reset": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase db reset",
    "supabase:migrate": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase migration up",
    "supabase:migration:new": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase migration new",
    "supabase:migration:list": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase migration list",
    "supabase:push": "bash scripts/check-supabase-env.sh --auto-unlink --force && npx supabase db push",
    "supabase:ensure-local": "bash scripts/check-supabase-env.sh --auto-unlink --force"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx}": [
      "bash -c 'tsc -p tsconfig.app.json --noEmit'"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "i18next": "^25.6.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^16.2.4",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.4.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}